<nav class="navbar navbar-default" role="navigation">
  <div class="container">
    <div class="navbar-header">
      <a class="navbar-brand" href="#"><b>Find a pair</b></a>
      <a class="navbar-brand" href="#"><b>Votes history</b></a>
    </div>
  </div>
</nav>
<div class="container">
  <div class="row" style="margin-top: 25px;">
    <div class="col-lg-10 col-lg-offset-1">
      <div class="well" >
        <div class="row">
          <div class="col-lg-12">
            <table class="table table-condensed">
              <thead>
                <tr>
                  <th>Photo</th>
                  <th>Email</th>
                  <th>Date</th>
                </tr>
              </thead>
              <% @votes.each do |vote| %>
                <tr class="<%= vote.like? ? 'success' : 'danger' %>">
                  <td><%= image_tag vote.media.file_name.preview.url  %></td>
                  <td><%= vote.media_user.email %></td>
                  <td><%= vote.created_at.strftime("%Y-%m-%d %H:%M") %></td>
                </tr>
              <% end %>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>